{
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "importOrder": [
        "^vue$",
        "^@inertiajs/.*",
        "^@/components/ui/.*",
        "^@iconify/vue$",
        "^@/composables/.*",
        "^@/components/.*",
        "^@/layouts/.*",
        "^@/pages/.*",
        "^@/.*",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ]
}
